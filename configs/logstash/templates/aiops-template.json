{
  "index_patterns": ["aiops-*"],
  "version": 1,
  "priority": 100,
  "template": {
    "settings": {
      "index": {
        "number_of_shards": 1,
        "number_of_replicas": 0,
        "refresh_interval": "5s",
        "codec": "best_compression",
        "max_result_window": 50000,
        "max_docvalue_fields_search": 200,
        "max_script_fields": 32,
        "max_inner_result_window": 100,
        "max_terms_count": 65536,
        "max_regex_length": 1000,
        "query": {
          "default_field": "message"
        },
        "mapping": {
          "total_fields": {
            "limit": 2000
          },
          "depth": {
            "limit": 20
          },
          "nested_fields": {
            "limit": 100
          },
          "nested_objects": {
            "limit": 10000
          },
          "field_name_length": {
            "limit": 1000
          }
        },
        "analysis": {
          "analyzer": {
            "aiops_analyzer": {
              "type": "custom",
              "tokenizer": "standard",
              "filter": [
                "lowercase",
                "stop",
                "snowball"
              ]
            },
            "log_analyzer": {
              "type": "custom",
              "tokenizer": "keyword",
              "filter": [
                "lowercase"
              ]
            },
            "path_analyzer": {
              "type": "custom",
              "tokenizer": "path_hierarchy",
              "filter": [
                "lowercase"
              ]
            }
          },
          "tokenizer": {
            "path_hierarchy": {
              "type": "path_hierarchy",
              "delimiter": "/"
            }
          },
          "normalizer": {
            "lowercase_normalizer": {
              "type": "custom",
              "filter": [
                "lowercase"
              ]
            }
          }
        },
        "lifecycle": {
          "name": "aiops-policy",
          "rollover_alias": "aiops-write"
        }
      }
    },
    "mappings": {
      "dynamic_templates": [
        {
          "message_field": {
            "path_match": "message",
            "match_mapping_type": "string",
            "mapping": {
              "type": "text",
              "analyzer": "aiops_analyzer",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256
                }
              }
            }
          }
        },
        {
          "string_fields": {
            "match": "*",
            "match_mapping_type": "string",
            "mapping": {
              "type": "text",
              "analyzer": "aiops_analyzer",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 256,
                  "normalizer": "lowercase_normalizer"
                }
              }
            }
          }
        },
        {
          "long_fields": {
            "match": "*",
            "match_mapping_type": "long",
            "mapping": {
              "type": "long"
            }
          }
        },
        {
          "double_fields": {
            "match": "*",
            "match_mapping_type": "double",
            "mapping": {
              "type": "double"
            }
          }
        },
        {
          "date_fields": {
            "match": "*",
            "match_mapping_type": "date",
            "mapping": {
              "type": "date"
            }
          }
        },
        {
          "geo_point_fields": {
            "match": "geoip.location",
            "mapping": {
              "type": "geo_point"
            }
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis"
        },
        "@version": {
          "type": "keyword"
        },
        "message": {
          "type": "text",
          "analyzer": "aiops_analyzer",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 8192
            },
            "raw": {
              "type": "keyword",
              "index": false
            }
          }
        },
        "log_level": {
          "type": "keyword",
          "normalizer": "lowercase_normalizer"
        },
        "log_level_normalized": {
